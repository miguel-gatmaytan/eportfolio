{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
    "assets/*": ["./src/assets/*"],
      "components/*": ["./src/components/*"],
      "constants/*": ["./src/constants/*"],
      "modules/*": ["./src/modules/*"]
    },
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "sourceMap": true
  },
  "include": ["./src/**/*", "./types/*"]
}
